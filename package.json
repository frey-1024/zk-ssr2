{
    "name": "react-ssr-music",
    "version": "1.0.0",
    "description": "使用react+koa的音乐同构项目",
    "main": "main.js",
    "scripts": {
        "debug": "cross-env NODE_ENV=development nodemon ./server/app.js",
        "debug:dev": "cross-env cross-env REACT_APP_ENV=dev npm run debug",
        "debug:test": "cross-env cross-env REACT_APP_ENV=test npm run debug",
        "debug:move": "cross-env cross-env REACT_APP_ENV=move npm run debug",
        "clear": "rimraf build",
        "build:client": "cross-env NODE_ENV=production webpack --progress --hide-modules --config config/webpack.client.config.js",
        "build:server": "cross-env NODE_ENV=production webpack --progress --hide-modules --config config/webpack.server.config.js",
        "build": "npm run build:client && npm run build:server",
        "build:dev": "npm run clear && cross-env NODE_ENV=production& cross-env REACT_APP_ENV=dev npm run build",
        "build:test": "npm run clear && cross-env NODE_ENV=production& cross-env REACT_APP_ENV=test npm run build",
        "build:move": "npm run clear && cross-env NODE_ENV=production& cross-env REACT_APP_ENV=move npm run build",
        "build:prod": "npm run clear && cross-env NODE_ENV=production& cross-env REACT_APP_ENV=prod npm run build",
        "server:dev": "pm2 startOrReload ecosystem.config.js --env dev",
        "server:test": "pm2 startOrReload ecosystem.config.js --env test",
        "server:move": "pm2 startOrReload ecosystem.config.js --env move",
        "server:prod": "pm2 startOrReload ecosystem.config.js --env prod",
        "server:stop": "pm2 stop ecosystem.config.js"
    },
    "keywords": [
        "react",
        "koa",
        "ssr",
        "直出"
    ],
    "author": "zhengw",
    "license": "ISC",
    "dependencies": {
